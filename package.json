    "firebase": "^10.11.1",
    "history": "^5.3.0",
    "image-capture": "^0.4.0",
    "keyrune": "^3.14.0",
    "mana-font": "^1.15.9",
    "preact": "^10.21.0",
    "preact-markup": "^2.1.1",
    "preact-router": "^4.1.2",
    "react-redux": "^9.2.0",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "rxjs": "^7.8.1",
    "scryfall-sdk": "^4.2.2",
    "tesseract.js": "^4.0.3",
    "tippy.js": "^6.3.7"
  },
  "resolutions": {
    "har-validator": "5.1.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.6"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@preact/preset-vite": "^2.9.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.10",
    "@types/react-redux": "^7.1.34",
    "@types/w3c-image-capture": "^1.0.10",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "chokidar": "^3.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "firebase-tools": "^13.8.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "preact-jsx-chai": "^3.0.0",
    "prettier": "^3.4.2",
    "sass": "^1.77.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2",
    "vite": "^6.0.7",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "jest": {
    "verbose": true,
    "preset": "ts-jest",
    "roots": [
      "<rootDir>"
    ],
    "setupFiles": [
      "<rootDir>/test/test-setup.ts"
    ],
    "testEnvironmentOptions": {
      "url": "http://localhost:8080"
    }
  }
}
